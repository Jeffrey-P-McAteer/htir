
# TODO: cargo respects values in here for _all_ operating systems,
# but the commented values here are only good for linux -> cross compile targets.
# Q: Can we specify these host-dependent? ie on linux use the following but on macos/windows do not?

[target.x86_64-apple-darwin]
linker = "o64-clang"
ar = "x86_64-apple-darwin20.4-ar"

[target.x86_64-pc-windows-gnu]
linker = "x86_64-w64-mingw32-gcc"
ar = "x86_64-w64-mingw32-ar"

